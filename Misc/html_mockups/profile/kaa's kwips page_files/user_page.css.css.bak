	BORDER-RIGHT: #a7d4ff 5px solid; PADDING-RIGHT: 16px; BORDER-TOP: #a7d4ff 5px solid; DISPLAY: none; PADDING-LEFT: 16px; Z-INDEX: 1002; LEFT: 25%; PADDING-BOTTOM: 16px; OVERFLOW: auto; BORDER-LEFT: #a7d4ff 5px solid; WIDTH: 30%; PADDING-TOP: 16px; BORDER-BOTTOM: #a7d4ff 5px solid; POSITION: absolute; TOP: 25%; HEIGHT: 20%; BACKGROUND-COLOR: white
